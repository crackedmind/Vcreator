    ],
    \"Category\" : \"Other Languages\",
    \"Description\" : \"Plugin for supporting the V programming language.\",
    \"Url\" : \"https://vlang.io\",
    $$dependencyList,

    \"Mimetypes\" : [
        \"<?xml version=\'1.0\'?>\",
        \"<mime-info xmlns=\'http://www.freedesktop.org/standards/shared-mime-info\'>\",
        \"    <mime-type type=\'text/x-vlang-project\'>\",
        \"        <sub-class-of type=\'text/plain\'/>\",
        \"        <comment>V project file</comment>\",
        \"        <glob pattern=\'v.mod\'/>\",
        \"    </mime-type>\",

        \"    <mime-type type=\'application/x-vlang\'>\",
        \"        <sub-class-of type=\'text/plain\'/>\",
        \"        <comment>V source file</comment>\",
        \"        <glob pattern=\'*.v\' weight=\'70\'/>\",
        \"        <glob pattern=\'*.vv\' weight=\'70\'/>\",
        \"        <glob pattern=\'*.vh\' weight=\'70\'/>\",
        \"        <glob pattern=\'*.vsh\' weight=\'70\'/>\",
        \"    </mime-type>\",
        \"</mime-info>\"
    ]
}
